#!/usr/bin/env php
<?php
require __DIR__ . '/../vendor/autoload.php';

use Autobahn\Cli\Application;
use Autobahn\Cli\Commands\EnvCommand;

// Setup application
$application = new Application('Autobahn CLI', '1.0.0-alpha1');

// Add commands
$application->add(new EnvCommand());

// Run application
$application->run();
